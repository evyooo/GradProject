<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout11"
    android:layout_width="154dp"
    android:layout_height="wrap_content"
    android:layout_marginLeft="2dp"
    android:layout_marginRight="2dp"
    android:background="@drawable/pink"
    android:orientation="vertical">

    <TextView
        android:id="@+id/postdate_textview_board"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="14dp"
        android:fontFamily="@font/regularnanumsquareround"
        android:text="TextView"
        android:textColor="@color/black"
        android:textSize="10dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/more_imageview_board"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="5dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/icon_more" />

    <TextView
        android:id="@+id/content_textview_board"
        android:layout_width="0dp"
        android:layout_height="100dp"
        android:layout_marginTop="9dp"
        android:layout_marginEnd="16dp"
        android:fontFamily="@font/regularnanumsquareround"
        android:text="TextView"
        android:textColor="@color/black"
        android:textSize="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/postdate_textview_board"
        app:layout_constraintTop_toBottomOf="@+id/postdate_textview_board" />

    <TextView
        android:id="@+id/writtenby_textview_board"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="21dp"
        android:fontFamily="@font/regularnanumsquareround"
        android:text="TextView"
        android:textColor="@color/black"
        android:textSize="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/content_textview_board"
        app:layout_constraintTop_toBottomOf="@+id/content_textview_board"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:id="@+id/fix_imageview_board"
        android:layout_width="14dp"
        android:layout_height="14dp"
        android:layout_marginEnd="15dp"
        app:layout_constraintBottom_toBottomOf="@+id/writtenby_textview_board"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/writtenby_textview_board"
        app:srcCompat="@drawable/fixed" />
</androidx.constraintlayout.widget.ConstraintLayout>